@prefix archive: <http://archive.example.org/> .
@prefix : <archive:vocab#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .


<.> a :Channel ;
    rdf:describedBy <> ;
    :hasFilter <.#resize_and_ocr> .

        
<.#resize_and_ocr> a :FilterChain ;
    :triggeredByType "image/jp2" ;
    :filter ([
        :name "resize" ;
        :type "modify" ;
        :arguments "--max_width 2048" ;
    ] [
        :type "meta" ;
        :name "extract_meta"
    ] [ 
        :type "extract" 
    ]) . 

